<h2 class="index-h2">ポイントランキング</h2>
<h4>ranking</h4>
<% @rankings.each do |ranking| %>
<p class="index-day">
  <%= ranking.created_at.strftime("%Y-%m-%d") %>
</p>
<p class="index-content">
  <%= link_to "関西学生ポイントランキング(" + ranking.created_at.strftime("%Y-%m-%d") + ")", ranking %>
</p>
<% end %>
